{"table":"sites","created":1436282308730,"url":"http://www.onthesnow.com/austria/ski-resorts.html","javascript":"window.links = [];var url = 'http://localhost:8000/rest/';var cbAjax = function() {    console.table(arguments);};$('#main .name a').each(function(id, link) {    links.push(link.href);});posts = [];var function_fetch = function(link) {    $.get(link).then(function(r) {        var $container = $(r).find('#main');        var $m = $container.find('.module');        var header = $container.find('.resort_name').html();        var content = '';        $m.find('#profile p').each(function(idxx, p) {            content += p.innerHTML;        });        var resort_elevation = $m.find('#resort_elevation').parent().html();        var resort_lifts = $m.find('#resort_lifts').parent().html();        var resort_terrain = $m.find('#resort_terrain').parent().html();        var resort_contact = $m.find('#resort_contact').parent().html();        var resort_trail = $container.find('.resort_overview .sbox img')[0].src + '|large|xlarge';        var map_url = $m.find('#map_canvas a img')[0].src;        var post = {            link: link,            content: content,            header: header,            resort_elevation: resort_elevation,            resort_lifts: resort_lifts,            resort_terrain: resort_terrain,            resort_contact: resort_contact,            resort_trail: resort_trail,            map_url: map_url,        };        posts.push(post);    });};var interval;interval = setInterval(function() {    if (window.links.length < 1) {        console.log('Last one');        clearInterval(interval);         console.log('There are %s posts', posts.length);        $.post(url + 'posts', {url:sites.url, posts: posts}, cbAjax);    }    var b = window.links.splice(0, 10);    $.each(b, function(idx, link) {        function_fetch(link);    });}, 10101);var sites = {url: window.location.href,javascript: actions};$.post(url + 'sites', sites, cbAjax);","_id":"FrtmZq99ClGBdIZ